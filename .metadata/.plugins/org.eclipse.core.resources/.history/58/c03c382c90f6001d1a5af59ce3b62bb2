#include <float.h>
#include <stdint.h>

#include "NORM_interface.h"
#include "NORM_private.h"


float NORM_f32_SlopeSignChange(uint32_t Copy_u32SSC)
{
	float Local_f32_NormSSC;

	Local_f32_NormSSC = (Copy_u32SSC - SNS1_SSC_MEAN) / SNS1_SSC_SD;

	return Local_f32_NormSSC;

}

float NORM_f32_ZeroCrossing(uint32_t Copy_u32zeroCross);

float NORM_f32_WaveformLength(uint32_t Copy_u3WaveLength);

float NORM_f32_RMS(float Copy_u32RMS);

float NORM_f32_Variance(float Copy_u32Variance);

float NORM_f32_Mean(float Copy_u32Mean);




