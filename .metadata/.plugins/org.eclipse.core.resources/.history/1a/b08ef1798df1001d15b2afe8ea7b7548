#include <stdint.h>

#define SRAM_SART		0x20000000U
#define SRAM_SIZE		(128*1024)U

#define SRAM_END		SRAM_SART + SRAM_SIZE



int Add(int A ,int B ,int C , int D)
{
	return A+B+C+D;

}

int main(void)
{
	int result;

	result = Add(1, 2,3, 4);

    /* Loop forever */
	for(;;);
}
