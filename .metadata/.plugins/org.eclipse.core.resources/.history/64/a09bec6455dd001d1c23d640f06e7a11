#ifndef RRC_H_
#define RRC_H_


#define HSE_CLK		1
#define HSI_CLK		2
#define PLL_CLK		3


#define ON			1
#define OFF			2

#define DEV_BY_1		1
#define DEV_BY_2		2

/*ClockTpye :
 * 1. HSE_CLK
 * 2. HSI_CLK
 * 3. PLL_CLK
 *
 * Status :
 * 1. ON
 * 2. OFF
 * */
int8_t RCC_Int8SetClockSrc(int8_t Copy_int8ClockType , int8_t Copy_int8Status);

int8_t RCC_Int8SetSysClock(int8_t Copy_int8ClockType);

/*DivisionScale :
 * 1. DEV_BY_1
 * 2. DEV_BY_2
 * */
int8_t RCC_Int8HSEConfig(int8_t Copy_int8DivisionScale);

#endif
